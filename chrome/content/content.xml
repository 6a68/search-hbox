<?xml version="1.0"?>
<bindings id="hboxy"
          xmlns="http://www.mozilla.org/xbl"
          xmlns:html="http://www.w3.org/1999/xhtml"
          xmlns:xul="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
          xmlns:xbl="http://www.mozilla.org/xbl">
  <binding id="hboxy-urlbar-rich-result-popup" extends="chrome://browser/content/urlbarBindings.xml#urlbar-rich-result-popup">
    <content ignorekeys="true"
             level="top"
             consumeoutsideclicks="never">
      <xul:hbox anonid="foomaster" style="text-align:left">
        <xul:description id="foomazing">this is the hbox, but it's not in the scroll box, and figuring out key focus seems problematic.</xul:description>
      </xul:hbox>
      <xul:richlistbox anonid="richlistbox" class="autocomplete-richlistbox" flex="1"/>
      <children />
    </content>
  </binding>
</bindings>

