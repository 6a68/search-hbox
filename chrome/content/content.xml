<?xml version="1.0"?>
<bindings id="hboxy"
          xmlns="http://www.mozilla.org/xbl"
          xmlns:html="http://www.w3.org/1999/xhtml"
          xmlns:xul="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
          xmlns:xbl="http://www.mozilla.org/xbl">
  <binding id="hboxy-urlbar-rich-result-popup" extends="chrome://browser/content/urlbarBindings.xml#urlbar-rich-result-popup">
    <content ignorekeys="true"
             level="top"
             consumeoutsideclicks="never">
      <xul:hbox anonid="foomaster">
        <xul:spacer flex="1"/>
        <xul:description anonid="foomazing" value="this is the thing"></xul:description>
      </xul:hbox>
      <children />
    </content>
  </binding>
</bindings>

